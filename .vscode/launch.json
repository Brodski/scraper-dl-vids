            "python": "${workspaceFolder}/SSScrapey-rework/src/venvFaster/Scripts/python",
            "cwd": "${workspaceFolder}/SSScrapey-rework/src",
            "args": [
                // "--quick-dl",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/app.py",
            "console": "integratedTerminal"
        }
