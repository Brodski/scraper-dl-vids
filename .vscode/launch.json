    "version": "0.2.0",
    "configurations": [
        {
            "name": "Preper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/SSScrapey-rework/src/kickPreper.py",
            "python": "${workspaceFolder}/SSScrapey-rework/src/venvFaster/Scripts/python",
            "cwd": "${workspaceFolder}/SSScrapey-rework/src",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--env",
                "local"
            ],
        },
        {
            "name": "Downloader",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/SSScrapey-rework/src/kickDownloader.py",
            "python": "${workspaceFolder}/SSScrapey-rework/src/venvFaster/Scripts/python",
            "cwd": "${workspaceFolder}/SSScrapey-rework/src",
            "args": [
                "--env",
                "local",
                "--dwn_batch_size",
                "2",
                // "--dwn_is_short_dev_dl"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Transcriber",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/SSScrapey-rework/src/kickTranscriber.py",
            "python": "${workspaceFolder}/SSScrapey-rework/src/venvFaster/Scripts/python",
            "cwd": "${workspaceFolder}/SSScrapey-rework/src",
            "args": [
                "--env",
                "local",
                "--transcriber_vods_per_instance",
                "2"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Splitter",
            "type": "debugpy",
            "request": "launch",
