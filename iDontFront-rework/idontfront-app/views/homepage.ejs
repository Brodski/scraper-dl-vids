<% let title = "Big Dick Captions 😏" %>

<!DOCTYPE html>
<html>
    <head>
        <title> <%= title %> </title>
        <%- include('partials/head_meta.ejs') %> 
    </head>
    <body class="homepage">
        <h1>Welcome to <%= title %>™ </h1>
        <h2> Captions available for these channels 🍆💦 : </h2>
        <div>
            <% 
            for (let chann of channelsList) {
                let nameId = chann.nameId;
                let logo = chann.logo
                let lang = chann.language
                let displayName = chann.displayName
                %>  
                <div class="channel_row img">
                    <a class="d-flex" href="<%= "channel/" + nameId %>">
                        <img class="" src="<%= chann.logo %>" />
                        <h2> <%= displayName %>  </h2>
                    </a>
                </div>
            <% } %> 
        </div>
    </body>
</html>
